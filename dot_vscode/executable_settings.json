{
  "animations.Active": "Indent",
  "animations.Command-Palette": "Fade",
  "animations.Enabled": true,
  "animations.Install-Method": "Apc Customize UI++",
  "animations.Scrolling": "Fade",
  "apc.activityBar": {
    "size": 30
  },
  "apc.imports": [
    "file:///c:/Users/tonyb/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.3/dist/updateHandler.js"
  ],
  "apc.listRow": {
    "fontSize": 14,
    "height": 30,
    "lists": ["explorer-folders-view", "quick-input-list", "results"]
  },
  "apc.statusBar": {
    "position": "editor-bottom"
  },
  "apc.stylesheet": {
    ".editor-actions": "display: none !important",
    ".editor-widget.suggest-widget": "border: 1px solid #FFCC66",
    ".editor-widget.suggest-widget.shows-details.visible": "border: none",
    // file icon alignment in search
    ".filematch .monaco-icon-label:before, .custom-list-row.results .monaco-list-row .monaco-icon-label:before": "background-position: 0 bottom !important",
    ".monaco-editor .scroll-decoration": "display: none !important",
    ".monaco-tl-row .monaco-tl-contents > div::after": "margin-bottom: auto",
    ".quick-input-list-entry .monaco-action-bar.quick-input-list-entry-action-bar": "align-self: center !important",
    ".quick-input-list-row div::before": "align-self: center !important",
    ".quick-input-widget": "transform: translateY(-50%) !important; top: 50% !important;",
    ".sidebar": "border: none !important",
    ".sidebar .title-label": "padding: 0 !important",
    ".sidebar .title-label h2": "line-height: 2.8 !important",
    ".suggest-details": "border: 1px solid #FFCC66 !important"
  },
  "catppuccin-icons.hidesExplorerArrows": true,
  "cody.autocomplete.formatOnAccept": true,
  "cody.commandCodeLenses": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'MesloLGS Nerd Font', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.lightbulb.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "magit.forge-enabled": true,
  "magit.quick-switch-enabled": true,
  "remote.autoForwardPortsSource": "hybrid",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
